<p id="notice"><%= notice %></p>

<h1>Tranos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tranos.each do |trano| %>
      <tr>
        <td><%= link_to 'Show', trano %></td>
        <td><%= link_to 'Edit', edit_trano_path(trano) %></td>
        <td><%= link_to 'Destroy', trano, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Trano', new_trano_path %>
